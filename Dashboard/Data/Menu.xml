    <DefaultAlias EXPKIT4_Tagline_5_0="Hide" EXPKIT4_Tagline_5_1="Hide" EXPKIT4_Tagline_5_2="Hide" EXPKIT4_Tagline_5_3="Hide" EXPKIT4_Tagline_5_4="Hide" EXPKIT4_Tagline_5_5="Hide" EXPKIT4_Tagline_5_6="Hide" EXPKIT4_Tagline_5_7="Hide"/>
   </AliasList>
  <Grid vgap="15" Align="Center">
    <Widget File="Spacer.xml" row="0" column="0" Width="20" Height="1">
      <StyleOverride>
        <Item>-fx-background-color: transparent;</Item>
      </StyleOverride>
    </Widget>
    <!-- Information area -->
    <Grid row="0" column="1" Width="$(Menu.Width)" Height="390">
      <StyleOverride>
        <Item>
          -fx-background-color: $(blue-80);
          -fx-border-radius: 10 10 10 10;
          -fx-background-radius: 10 10 10 10;
          -fx-border-color: $(orange);
          -fx-border-width: 3;
        </Item>
      </StyleOverride>
      <Widget File="Text/Text.xml" row="0" column="0" Align="N">
        <InitialValue>Information Area</InitialValue>
        <StyleOverride>
          <Item>
            -fx-font-family:$(Menu.Title.Text.Font);
            -fx-font-size: 	$(Menu.Title.Text.Size);
            -fx-text-fill: 	$(white);
          </Item>
        </StyleOverride>
      </Widget>
      <DynamicGrid row="1" column="0" Width="$(Menu.Width)" Height="390" Align="N" >
        <GridFile Source="$(AppDir)/Menu.Buttons.xml" ID="Main" Area="Info" PageID="Main" ButtonsCount="8">
          <Transition Delay="100" Duration="500">CUBE</Transition>
        </GridFile>
        <For Count="8" StartValue="1">
          <GridFile Source="$(AppDir)/Menu.Buttons.xml" ID="$(currentvaluealias)" Area="Info" PageID="$(currentvaluealias)" ButtonsCount="10">
            <Transition Delay="100" Duration="500">CUBE</Transition>
          </GridFile>
        </For>
        <Initial ID="Main" />
        <MinionSrc ID="Info.PageID" Namespace="Menu" />
      </DynamicGrid>
    </Grid>
    <!-- Demo area -->
    <Grid row="1" column="1" Width="$(Menu.Width)" Height="330">
      <StyleOverride>
        <Item>
          -fx-background-color: $(blue-80);
          -fx-border-radius: 10 10 10 10;
          -fx-background-radius: 10 10 10 10;
          -fx-border-color: $(green);
          -fx-border-width: 3;
        </Item>
      </StyleOverride>
      <Widget File="Text/Text.xml" row="0" column="0" Align="N">
        <InitialValue>Demo Area</InitialValue>
        <StyleOverride>
          <Item>
            -fx-font-family:$(Menu.Title.Text.Font);
            -fx-font-size: 	$(Menu.Title.Text.Size);
            -fx-text-fill: 	$(white);
            -fx-wrap-text:true;
          </Item>
        </StyleOverride>
      </Widget>
      <DynamicGrid row="1" column="0" Width="$(Menu.Width)" Height="330" Align="N">
        <GridFile Source="$(AppDir)/Menu.Buttons.xml" ID="Main" Area="Demo" PageID="Main" ButtonsCount="8">
          <Transition Delay="100" Duration="500">CUBE</Transition>
        </GridFile>
        <For Count="8" StartValue="1">
          <GridFile Source="$(AppDir)/Menu.Buttons.xml" ID="$(currentvaluealias)" Area="Demo" PageID="$(currentvaluealias)" ButtonsCount="8">
            <Transition Delay="100" Duration="500">CUBE</Transition>
          </GridFile>
        </For>
        <Initial ID="Main" />
        <MinionSrc ID="Demo.PageID" Namespace="Menu" />
      </DynamicGrid>
    </Grid>
    <!-- Legal area -->
    <Grid row="2" column="1" Width="$(Menu.Width)" Height="130">
      <StyleOverride>
        <Item>
          -fx-background-color: $(blue-80);
          -fx-border-radius: 10 10 10 10;
          -fx-background-radius: 10 10 10 10;
          -fx-border-color: $(blue);
          -fx-border-width: 3;
        </Item>
      </StyleOverride>
      <DynamicGrid row="1" column="0" Width="$(Menu.Width)" Height="130" Align="S">
        <GridFile Source="$(AppDir)/Menu.Buttons.xml" ID="Main" Area="Legal" PageID="Main" ButtonsCount="8">
          <Transition Delay="100" Duration="500">CUBE</Transition>
        </GridFile>
        <For Count="2" StartValue="1">
          <GridFile Source="$(AppDir)/Menu.Buttons.xml" ID="$(currentvaluealias)" Area="Legal" PageID="$(currentvaluealias)" ButtonsCount="8">
            <Transition Delay="100" Duration="500">CUBE</Transition>
          </GridFile>
        </For>
        <Initial ID="Main" />
        <MinionSrc ID="Legal.PageID" Namespace="Menu" />
      </DynamicGrid>
    </Grid>
  </Grid>
  <!-- Tasks -->
  <!-- Info area -->
  <!-- Button Reset Task -->
  <TaskList ID="ResetButtons">
    <TaskItem Type="Marvin">
      <DataToInsert ID="Deselect" Namespace="Button.Deselect" Data="Deselect"/>
    </TaskItem>
  </TaskList>
  <!-- Common back button -->
  <TaskList ID="Button.Task.Info.Back">
    <TaskItem Type="OtherTask" ID="ResetButtons" />
    <TaskItem Type="Marvin">
      <DataToInsert ID="Info.PageID" Namespace="Menu" Data="Main" />
    </TaskItem>
    <TaskItem Type="Marvin">
      <DataToInsert ID="Info.Slide" Namespace="Menu" Data="Default" />
    </TaskItem>
    <TaskItem Type="Marvin">
      <DataToInsert ID="Area" Namespace="Menu" Data="Info" />
    </TaskItem>
    <TaskItem Type="Marvin">
      <DataToInsert ID="TitleVisibility" Namespace="Page" Data="Hide" />
    </TaskItem>
  </TaskList>

  <!-- Main -->
  <For Count="8" StartValue="1">
    <TaskList ID="Button.Task.Info.Main.$(currentvaluealias)">
      <TaskItem Type="OtherTask" ID="ResetButtons" />
      <TaskItem Type="Marvin">
        <DataToInsert ID="Info.PageID" Namespace="Menu" Data="$(currentvaluealias)" />
      </TaskItem>
      <TaskItem Type="Marvin">
        <DataToInsert ID="SubpageTitle" Namespace="Page" Data="$(Button.Title.Info.Main.$(currentvaluealias).$(INFO.SUBMENU_$(currentvaluealias)_DEFAULT))" />
      </TaskItem>
      <TaskItem Type="Marvin">
        <DataToInsert ID="Title" Namespace="Info" Data="$(Button.Text.Info.Main.$(currentvaluealias))" />
      </TaskItem>
      <TaskItem Type="Marvin" Postpone="1">
        <DataToInsert ID="Info.$(currentvaluealias).$(INFO.SUBMENU_$(currentvaluealias)_DEFAULT)" Namespace="Button.Peekaboo" Data="Select"/>
      </TaskItem>
      <TaskItem Type="Marvin">
        <DataToInsert ID="TitleVisibility" Namespace="Page" Data="Show" />
      </TaskItem>
      <TaskItem Type="Marvin">
        <DataToInsert ID="SubpageTitleVisibility" Namespace="Info" Data="Show" />
      </TaskItem>
      <TaskItem Type="Marvin">
        <DataToInsert ID="Info.Slide" Namespace="Menu" Data="$(INFO.SUBMENU_$(currentvaluealias)_DEFAULT)" />
      </TaskItem>
      <TaskItem Type="Marvin">
        <DataToInsert ID="Area" Namespace="Menu" Data="Info" />
      </TaskItem>
    </TaskList>
  </For>

  <!-- Info Menu Items Tasklist -->
  <For Count="8" StartValue="1" CurrentValueAlias="outervaluealias">
    <For Count="10" StartValue="1">
      <TaskList ID="Button.Task.Info.$(outervaluealias).$(currentvaluealias)">
        <TaskItem Type="OtherTask" ID="ResetButtons" />
        <TaskItem Type="Marvin">
          <DataToInsert ID="Info.Slide" Namespace="Menu" Data="$(currentvaluealias)" />
        </TaskItem>
        <TaskItem Type="Marvin">
          <DataToInsert ID="SubpageTitle" Namespace="Page" Data="$(Button.Title.Info.Main.$(outervaluealias).$(currentvaluealias))" />
        </TaskItem>
        <TaskItem Type="Marvin" Postpone="1">
          <DataToInsert ID="Info.$(outervaluealias).$(currentvaluealias)" Namespace="Button.Peekaboo" Data="Select"/>
        </TaskItem>
        <TaskItem Type="Marvin">
          <DataToInsert ID="TitleVisibility" Namespace="Page" Data="Show" />
        </TaskItem>
        <TaskItem Type="Marvin">
          <DataToInsert ID="SubpageTitleVisibility" Namespace="Info" Data="Show" />
        </TaskItem>
        <TaskItem Type="Marvin">
          <DataToInsert ID="Area" Namespace="Menu" Data="Info" />
        </TaskItem>
      </TaskList>
    </For>
  </For>

  <!-- legal area -->
  <!-- Common back button -->
  <TaskList ID="Button.Task.Legal.Back">
    <TaskItem Type="Marvin">
      <DataToInsert ID="Legal.PageID" Namespace="Menu" Data="Main" />
    </TaskItem>
    <TaskItem Type="Marvin">
      <DataToInsert ID="Title" Namespace="Legal" Data="" />
    </TaskItem>
    <TaskItem Type="Marvin">
      <DataToInsert ID="Area" Namespace="Menu" Data="Legal" />
    </TaskItem>
    <TaskItem Type="Marvin">
      <DataToInsert ID="Legal.Slide" Namespace="Menu" Data="Default" />
    </TaskItem>
    <TaskItem Type="Marvin">
      <DataToInsert ID="TitleVisibility" Namespace="Page" Data="Hide" />
    </TaskItem>
  </TaskList>

  <!-- legal -->
  <For Count="8" StartValue="1">
    <TaskList ID="Button.Task.Legal.Main.$(currentvaluealias)">
      <TaskItem Type="OtherTask" ID="ResetButtons" />
      <TaskItem Type="Marvin">
        <DataToInsert ID="Legal.PageID" Namespace="Menu" Data="$(currentvaluealias)" />
      </TaskItem>
      <TaskItem Type="Marvin">
        <DataToInsert ID="SubpageTitle" Namespace="Page" Data="$(Button.Title.Legal.Main.$(currentvaluealias).$(INFO.SUBMENU_$(currentvaluealias)_DEFAULT))" />
      </TaskItem>
      <TaskItem Type="Marvin">
        <DataToInsert ID="Title" Namespace="Info" Data="$(Button.Text.Legal.Main.$(currentvaluealias))" />
      </TaskItem>
      <TaskItem Type="Marvin" Postpone="1">
        <DataToInsert ID="Legal.$(currentvaluealias).$(INFO.SUBMENU_$(currentvaluealias)_DEFAULT)" Namespace="Button.Peekaboo" Data="Select"/>
      </TaskItem>
      <TaskItem Type="Marvin">
        <DataToInsert ID="TitleVisibility" Namespace="Page" Data="Show" />
      </TaskItem>
      <TaskItem Type="Marvin">
        <DataToInsert ID="SubpageTitleVisibility" Namespace="Legal" Data="Show" />
      </TaskItem>
      <TaskItem Type="Marvin">
        <DataToInsert ID="Legal.Slide" Namespace="Menu" Data="$(INFO.SUBMENU_$(currentvaluealias)_DEFAULT)" />
      </TaskItem>
      <TaskItem Type="Marvin">
        <DataToInsert ID="Area" Namespace="Menu" Data="Legal" />
      </TaskItem>
    </TaskList>
  </For>

  <!-- Legal Menu Items Tasklist -->
  <For Count="8" StartValue="1" CurrentValueAlias="outervaluealias">
    <For Count="8" StartValue="1">
      <TaskList ID="Button.Task.Legal.$(outervaluealias).$(currentvaluealias)">
        <TaskItem Type="OtherTask" ID="ResetButtons" />
        <TaskItem Type="Marvin">
          <DataToInsert ID="Legal.Slide" Namespace="Menu" Data="$(currentvaluealias)" />
        </TaskItem>
        <TaskItem Type="Marvin">
          <DataToInsert ID="SubpageTitle" Namespace="Page" Data="$(Button.Title.Legal.Main.$(outervaluealias).$(currentvaluealias))" />
        </TaskItem>
        <TaskItem Type="Marvin" Postpone="1">
          <DataToInsert ID="Legal.$(outervaluealias).$(currentvaluealias)" Namespace="Button.Peekaboo" Data="Select"/>
        </TaskItem>
        <TaskItem Type="Marvin">
          <DataToInsert ID="TitleVisibility" Namespace="Page" Data="Show" />
        </TaskItem>
        <TaskItem Type="Marvin">
          <DataToInsert ID="SubpageTitleVisibility" Namespace="Legal" Data="Show" />
        </TaskItem>
        <TaskItem Type="Marvin">
          <DataToInsert ID="Area" Namespace="Menu" Data="Legal" />
        </TaskItem>
      </TaskList>
    </For>
  </For>

  <!-- Demo area -->
  <!-- Common back button -->
  <TaskList ID="Button.Task.Demo.Back">
    <TaskItem Type="Marvin">
      <DataToInsert ID="Demo.PageID" Namespace="Menu" Data="Main" />
    </TaskItem>
    <TaskItem Type="Marvin">
      <DataToInsert ID="Title" Namespace="Demo" Data="" />
    </TaskItem>
    <TaskItem Type="Marvin">
      <DataToInsert ID="Area" Namespace="Menu" Data="Demo" />
    </TaskItem>
    <TaskItem Type="Marvin">
      <DataToInsert ID="Info.Slide" Namespace="Menu" Data="Default" />
    </TaskItem>
    <TaskItem Type="Marvin">
      <DataToInsert ID="TitleVisibility" Namespace="Page" Data="Hide" />
    </TaskItem>
  </TaskList>

  <!-- Main -->
  <For Count="8" StartValue="1">
    <TaskList ID="Button.Task.Demo.Main.$(currentvaluealias)">
      <TaskItem Type="OtherTask" ID="ResetButtons" />
      <TaskItem Type="Marvin">
        <DataToInsert ID="Area" Namespace="Menu" Data="Demo" />
      </TaskItem>
      <TaskItem Type="Marvin">
        <DataToInsert ID="Demo.PageID" Namespace="Menu" Data="$(currentvaluealias)" />
      </TaskItem>
      <TaskItem Type="Marvin">
        <DataToInsert ID="Title" Namespace="Demo" Data="$(Button.Title.Demo.Main.$(currentvaluealias))" />
      </TaskItem>
      <If Value1="$(Button.Text.Demo.Main.$(currentvaluealias))" Compare="IF_EQ" Value2="Telemetry">
        <Then>
          <TaskItem Type="Marvin">
            <DataToInsert ID="TitleVisibility" Namespace="Page" Data="Hide" />
          </TaskItem>
        </Then>
        <Else>
          <TaskItem Type="Marvin">
            <DataToInsert ID="TitleVisibility" Namespace="Page" Data="Show" />
          </TaskItem>
        </Else>
      </If>
      <TaskItem Type="Marvin">
        <DataToInsert ID="SubpageTitleVisibility" Namespace="Demo" Data="Hide" />
      </TaskItem>
    </TaskList>
  </For>

  <!-- Demo 1 -->
  <For Count="8" StartValue="1">
    <TaskList ID="Button.Task.Demo.1.$(currentvaluealias)">
      <TaskItem Type="OtherTask" ID="ResetButtons" />
      <TaskItem Type="Oscar">
        <Task OscarID="Oscar.Main">StopLive</Task>
      </TaskItem>
      <TaskItem Type="Marvin">
        <DataToInsert ID="Title" Namespace="Demo" Data="$(Button.Title.Demo.Main.1)" />
      </TaskItem>
      <TaskItem Type="Oscar">
        <Task OscarID="Oscar.Main">Playback</Task>
        <Param>speed=1</Param>
        <Param>Loop</Param>
        <Param>File=$(EXPKIT2_BIFF_DATA_$(currentvaluealias))</Param>
      </TaskItem>
      <If Value1="$(currentvaluealias)" Compare="IF_LT" Value2="7">
        <Then>
          <TaskItem Type="Marvin" Postpone="1">
            <DataToInsert ID="Demo.1.$(currentvaluealias)" Namespace="Button.Peekaboo" Data="Select"/>
          </TaskItem>
          <TaskItem Type="Marvin">
            <DataToInsert ID="SubpageTitleVisibility" Namespace="Demo" Data="Show" />
          </TaskItem>
          <TaskItem Type="Marvin">
            <DataToInsert ID="SubpageTitle" Namespace="Page" Data="$(Button.Text.Demo.1.$(currentvaluealias))" />
          </TaskItem>
        </Then>
        <Else>
          <TaskItem Type="Marvin">
            <DataToInsert ID="SubpageTitleVisibility" Namespace="Demo" Data="Hide" />
          </TaskItem>
        </Else>
      </If>
      <If Value1="$(currentvaluealias)" Compare="IF_EQ" Value2="8">
        <Then>
          <TaskItem Type="LaunchProgram">
            <Application>explorer.exe</Application>
            <Param>"https://networkbuilders.intel.com/social-hub/video/enhanced-platform-awareness-in-kubernetes-demo-video"</Param>
          </TaskItem>
        </Then>
      </If>
      <TaskItem Type="Marvin">
        <DataToInsert Namespace="Expkit2" ID="Tagline" Data="$(EXPKIT2_Tagline_$(currentvaluealias)_0)" />
      </TaskItem>
      <TaskItem Type="Marvin">
        <DataToInsert Namespace="Expkit2" ID="Tagline.1" Data="$(EXPKIT2_Tagline_$(currentvaluealias)_1)" />
      </TaskItem>
      <TaskItem Type="Marvin">
        <DataToInsert Namespace="Expkit2" ID="Tagline.2" Data="$(EXPKIT2_Tagline_$(currentvaluealias)_2)" />
      </TaskItem>
      <TaskItem Type="Marvin">
        <DataToInsert Namespace="Expkit2" ID="Tagline.3" Data="$(EXPKIT2_Tagline_$(currentvaluealias)_3)" />
      </TaskItem>
      <TaskItem Type="Marvin">
        <DataToInsert Namespace="Expkit2" ID="Tagline.4" Data="$(EXPKIT2_Tagline_$(currentvaluealias)_4)" />
      </TaskItem>
      <TaskItem Type="Marvin">
        <DataToInsert Namespace="Expkit2" ID="Tagline.5" Data="$(EXPKIT2_Tagline_$(currentvaluealias)_5)" />
      </TaskItem>
      <TaskItem Type="Marvin">
        <DataToInsert Namespace="Expkit2" ID="Tagline.6" Data="$(EXPKIT2_Tagline_$(currentvaluealias)_6)" />
      </TaskItem>
      <TaskItem Type="Marvin">
        <DataToInsert ID="Area" Namespace="Menu" Data="Demo" />
      </TaskItem>
      <TaskItem Type="Marvin">
        <DataToInsert ID="TitleVisibility" Namespace="Page" Data="Show" />
      </TaskItem>
    </TaskList>
  </For>

  <!-- Demo 2 -->
  <For Count="8" StartValue="1">
    <TaskList ID="Button.Task.Demo.2.$(currentvaluealias)">
      <TaskItem Type="OtherTask" ID="ResetButtons" />
      <If Value1="$(currentvaluealias)" Compare="IF_LT" Value2="7">
        <Then>
          <TaskItem Type="Marvin" Postpone="1">
            <DataToInsert ID="Demo.2.$(currentvaluealias)" Namespace="Button.Peekaboo" Data="Select"/>
          </TaskItem>
          <TaskItem Type="Marvin">
            <DataToInsert ID="SubpageTitleVisibility" Namespace="Demo" Data="Show" />
          </TaskItem>
          <TaskItem Type="Marvin">
            <DataToInsert ID="SubpageTitle" Namespace="Page" Data="$(Button.Text.Demo.2.$(currentvaluealias))" />
          </TaskItem>
        </Then>
        <Else>
          <TaskItem Type="Marvin">
            <DataToInsert ID="SubpageTitleVisibility" Namespace="Demo" Data="Hide" />
          </TaskItem>
        </Else>
      </If>
      <If Value1="$(currentvaluealias)" Compare="IF_EQ" Value2="8">
        <Then>
          <TaskItem Type="LaunchProgram">
            <Application>explorer.exe</Application>
            <Param>"https://networkbuilders.intel.com/social-hub/video/doxsehe0"</Param>
          </TaskItem>
        </Then>
      </If>
      <TaskItem Type="Oscar">
        <Task OscarID="Oscar.Main">StopLive</Task>
      </TaskItem>
      <TaskItem Type="Oscar">
        <Task OscarID="Oscar.Main">Playback</Task>
        <Param>speed=1</Param>
        <Param>Loop</Param>
        <Param>File=$(EXPKIT3_BIFF_DATA_$(currentvaluealias))</Param>
      </TaskItem>
      <TaskItem Type="Marvin">
        <DataToInsert ID="Area" Namespace="Menu" Data="Demo" />
