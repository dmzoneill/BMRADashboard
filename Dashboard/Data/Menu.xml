    <DefaultAlias EXPKIT2_Tagline_7_0="Hide" EXPKIT2_Tagline_7_1="Hide" EXPKIT2_Tagline_7_2="Hide" EXPKIT2_Tagline_7_3="Hide" EXPKIT2_Tagline_7_4="Hide" EXPKIT2_Tagline_7_5="Hide" EXPKIT2_Tagline_7_6="Hide" EXPKIT2_Tagline_7_7="Hide"/>
    <!-- Demo 2 -->
    <DefaultAlias EXPKIT3_BIFF_DATA_1="data/Expkit3_64B.BIFF"/>
    <DefaultAlias EXPKIT3_BIFF_DATA_2="data/Expkit3_128B.BIFF"/>
    <DefaultAlias EXPKIT3_BIFF_DATA_3="data/Expkit3_256B.BIFF"/>
    <DefaultAlias EXPKIT3_BIFF_DATA_4="data/Expkit3_512B.BIFF"/>
    <DefaultAlias EXPKIT3_BIFF_DATA_5="data/Expkit3_1024B.BIFF"/>
    <DefaultAlias EXPKIT3_BIFF_DATA_6="data/Expkit3_1518B.BIFF"/>
    <DefaultAlias EXPKIT3_BIFF_DATA_7="data/Expkit3_notraffic.BIFF"/>
