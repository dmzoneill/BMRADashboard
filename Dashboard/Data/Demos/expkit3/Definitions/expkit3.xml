<MarvinExternalFile>
  <AliasList>
    <Alias GridID="Message" />
    <Alias CustomDir="$(AppDir)/Demos/expkit3" />
    <!-- Layout and colors -->
    <Alias demo.grid.height="500" demo.grid.width="1510" />
    <Alias demo.server.grid.width="400" demo.server.grid.height="510" demo.server.width="380" />
    <Alias findings.height="410" findings.width="745" />
    <Alias demo.container.title.background="$(light-blue)" demo.container.background="$(transparent)" />
    <Alias demo.pod.grid.width="1096" demo.pod.grid.height="510" demo.pod.grid.background="$(blue-40)" demo.pod.title.background="$(blue)" demo.pod.topology.width="1062" />
    <Alias demo.grid.font="&quot;Intel Clear Pro&quot;" />
    <Alias demo.information.grid.width="$(demo.grid.width)" demo.information.grid.height="218" demo.information.grid.background="$(blue)" demo.information.title.background="$(green)" demo.information.height="180" />
    <Alias demo.statspanel.height="0" demo.statspanel.width="500" />
    <Alias demo.gauge.width="160" demo.gauge.height="160" />
    <Alias demo.lcd.width="140" demo.lcd.height="90" />
    <Alias demo.panel.title.background="$(blue-gradient)" />
    <!-- Test and Messaging Section -->
    <Alias MinionID="expkit3.demo" />
    <!-- SUT 1 Specific -->
    <Alias SUT.NS.1.enabled="expkit3.DPDK" />
    <Alias SUT.NS.1.title="DPDK Application" />
    <!-- SUT 2 Specific -->
    <Alias SUT.NS.2.enabled="expkit3.Kernel" />
    <Alias SUT.NS.2.title="Linux TCP/IP stack application" />
    <!-- Minion Sources and Titles -->
    <Alias minion.source.1="PacketRate" />
    <Alias minion.source.2="Throughput" />
    <Alias minion.source.3="CPU" />
    <Alias minion.source.4="PacketLoss" />
  </AliasList>
</MarvinExternalFile>