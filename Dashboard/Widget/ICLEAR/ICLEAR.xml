<MarvinExternalFile>
  <AliasList>
    <Import>Widget/ICLEAR/ICLEAR-Colors.xml</Import>
    <Alias Default_LeftGap="0" Default_RightGap="0" Default_TopGap="0" Default_BottomGap="0" /> <!-- Space between compound pieces -->
    <Alias Default_LeftPad="0" Default_RightPad="0" Default_TopPad="0" Default_BottomPad="0" /> <!-- Space around compound grid -->
    <Alias DEFAULT_HORIZONTAL_BAR_HEIGHT="30" DEFAULT_VERTICAL_BAR_HEIGHT="30" />
    <Alias DEFAULT_HCAP_WIDTH="$(DEFAULT_VERTICAL_BAR_HEIGHT)" DEFAULT_VCAP_HEIGHT="$(DEFAULT_HORIZONTAL_BAR_HEIGHT)" />
    <Alias DEFAULT_BUTTON_HEIGHT="$(DEFAULT_HORIZONTAL_BAR_HEIGHT)" DEFAULT_BUTTON_WIDTH="150" />
    <Alias ICLEAR_FONT1="&quot;Intel Clear Pro&quot;" ICLEAR_FONT2="&quot;Intel Clear Pro&quot;" ICLEAR_FONT3="&quot;Intel Clear&quot;" Menu.Title.Text.Font="&quot;Intel Clear Pro&quot;" Menu.Title.Text.Size="32px" />
    <DefaultAlias TOP_GAP="$(Default_TopGap)" BOTTOM_GAP="$(Default_BottomGap)" LEFT_GAP="$(Default_LeftGap)" RIGHT_GAP="$(Default_RightGap)" />
    <DefaultAlias LEFT_PAD="$(Default_LeftPad)" RIGHT_PAD="$(Default_RightPad)" TOP_PAD="$(Default_LeftPad)" BOTTOM_PAD="$(Default_BottomPad)" />
    <DefaultAlias HORIZONTAL_BAR_HEIGHT="$(DEFAULT_HORIZONTAL_BAR_HEIGHT)" VERTICAL_BAR_WIDTH="$(DEFAULT_VERTICAL_BAR_HEIGHT)" />
    <DefaultAlias HCAP_WIDTH="$(DEFAULT_HCAP_WIDTH)" VCAP_HEIGHT="$(DEFAULT_VCAP_HEIGHT)" />
    <DefaultAlias ICLEAR_HOVER="NoHover" />
    <DefaultAlias ICLEAR_TEXT_SIZE="20px" ICLEAR_TEXT_FONT="$(ICLEAR_FONT2)" />
    <DefaultAlias ICLEAR_TASK="" />
    <DefaultAlias ICLEAR_NAMESPACE="unused" ICLEAR_ID="unused" />
    <DefaultAlias ICLEAR_PEEKABOO_NAMESPACE="unused" ICLEAR_PEEKABOO_ID="unused" />
    <DefaultAlias ICLEAR_CUSTOM_STYLE="" /> <!-- Basically Custom  Style override strings can be put in here -->
  </AliasList>
</MarvinExternalFile>